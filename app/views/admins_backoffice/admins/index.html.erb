<h1>Admins</h1>

<table class="table table-striped table-dark">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Email</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @admins.each do |admin|%>
        <tr>
        <th scope="row"><%= admin.id%></th>
        <td><%= admin.email%></td>
        <td><%= link_to 'Editar', edit_admins_backoffice_admin_path(admin)#transforma id especifico em parametro%></td>
        </tr>
    <% end %>
  </tbody>
</table>